{
    "name": "juniorcodinglab/middleware-tray-braspress",
    "description": "API Intermediadora que faz a ponte entre a cotação da Brasspress com a Plataforma Tray",
    "keywords": ["microframework", "rest", "router", "psr7"],
    "homepage": "http://github.com/slimphp/Slim-Skeleton",
    "license": "-",
    "authors": [
        {
            "name": "Silvio Junior",
            "email": "silvio_cbsj@hotmail.com",
            "homepage": "https://manualdoprogramador.com/"
        }
    ],
    "require": {
        "php": ">=7.2",
        "slim/slim": "3.*",
        "monolog/monolog": "^1.17",
        "vlucas/phpdotenv": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "config": {
        "process-timeout" : 0,
        "optimize-autoloader": true
    },
    "scripts": {
        "start": "php -S localhost:8080 -t /"
    }

}
